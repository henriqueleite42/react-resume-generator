{
  "name": "react-resume-generator",
  "homepage": "http://henriqueleite42.github.io/react-resume-generator/",
  "version": "0.1.0",
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "tsc && lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "npx prettier --write",
      "eslint src/*.tsx --fix-dry-run"
    ]
  },
  "dependencies": {
    "blob-stream": "^0.1.3",
    "pdfkit": "^0.11.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-easy-crop": "^3.1.0",
    "react-hook-form": "^6.0.5",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.1",
    "react-textarea-autosize": "^8.2.0",
    "redux": "^4.0.5",
    "styled-components": "^5.1.1",
    "sweetalert2": "^9.10.12"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/blob-stream": "^0.1.30",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
    "@types/pdfkit": "^0.10.6",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/react-easy-crop": "^1.16.0",
    "@types/react-image-crop": "^8.1.2",
    "@types/react-redux": "^7.1.7",
    "@types/react-router-dom": "^5.1.5",
    "@types/styled-components": "^5.1.0",
    "@typescript-eslint/eslint-plugin": "^2.22.0",
    "@typescript-eslint/parser": "^2.22.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import-helpers": "^1.0.2",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^4.0.4",
    "gh-pages": "^3.1.0",
    "husky": "^4.2.3",
    "lint-staged": "^10.1.2",
    "prettier": "^1.19.1",
    "typescript": "^3.8.3"
  }
}
